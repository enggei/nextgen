delimiters "~","~"

Block(lines) ::= <<~lines:{it|~it~};separator="\n"~  >>

Encapsulation(prefix,content,postfix) ::= <<~prefix~~content~~postfix~  >>

Line(line,children,end,next) ::= <<~line~~if(children)~
	~children:{it|~it~};separator="\n"~~endif~~if(end)~
~end~~endif~
~next~  >>

Sequence(items) ::= <<~items:{it|~it~};separator=" "~  >>

eom() ::= "}"

gt() ::= ">"
